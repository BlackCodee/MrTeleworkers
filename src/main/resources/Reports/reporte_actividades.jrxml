		<queryString language="SQL">
			<![CDATA[SELECT COUNT(*) AS Nro_Actividades, ae.nombre_area 
FROM `actividad_laboral` al INNER JOIN area_empresarial ae 
ON al.id_area = ae.id_area 
WHERE al.estado = 'activo' 
GROUP BY al.id_area]]>
		</queryString>
